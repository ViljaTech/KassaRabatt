    <style name="AppTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <!-- <item name="android:editTextBackground">@drawable/launch_screen</item> -->
        <item name="android:statusBarColor">#FDA228</item>
